					</div>
					<div class="col-md-4">
						<div class="input-group col-md-12">
						  <input type="text" class="form-control" placeholder="Preferred day*" aria-describedby="sizing-addon1">
						</div>
					</div>
					<div class="col-md-4">
						<div class="input-group col-md-12">
						  <input type="text" class="form-control" placeholder="Preferred time*" aria-describedby="sizing-addon1">
						</div>
					</div>
				</div>
				<div class="row margin-bottom-5">
					<div class="col-md-12">
						<div class="input-group col-md-12">
							 <md-autocomplete md-floating-label=""
			                      flex=""
			                      md-items="item in ctrl.data"
			                      md-item-text="item.display_name"
			                      md-search-text-change="ctrl.locLookUp(ctrl.searchText)"
			                      md-search-text="ctrl.searchText"
			                      md-selected-item="ctrl.selectedItem"
			                      md-no-cache="ctrl.noCache"
			                      md-input-maxlength="30"
			                      md-input-minlength="2"
			                      md-input-name="autocompleteField"
			                      required=""
			                      placeholder="Location"
			                      class="col-lg-12 md-input">

						        <md-item-template>
						          <span md-highlight-text="ctrl.searchText">{{item.display_name}}</span>
						        </md-item-template>
						    </md-autocomplete>
						</div>
					</div>
				</div>
				<div class="row margin-bottom-10">
					<div class="col-md-12">
						<textarea class="form-control" rows="5" placeholder="Message" id="comment"></textarea>
